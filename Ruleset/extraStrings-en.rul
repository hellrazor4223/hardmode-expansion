      STR_ARMORED_VEST: "Armored Vest"
      STR_ARMORED_VEST_UC: "ARMORED VEST"
      STR_ARMORED_VEST_CORPSE: "Damaged Armored Vest"
      STR_ARMORED_VEST_REPAIR: "Repair Armored Vest"
